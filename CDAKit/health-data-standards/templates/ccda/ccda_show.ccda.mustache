<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="WebViewLayout_CDA.xsl"?>
<ClinicalDocument xmlns="urn:hl7-org:v3" xmlns:sdtc="urn:hl7-org:sdtc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:cda="urn:hl7-org:v3" xsi:schemaLocation="urn:hl7-org:v3 http://xreg2.nist.gov:8080/hitspValidation/schema/cdar2c32/infrastructure/cda/C32_CDA.xsd">
  <realmCode code="US"/>
  <typeId root="2.16.840.1.113883.1.3" extension="POCD_HD000040"/>
  <templateId root="2.16.840.1.113883.10.20.22.1.1" />
  <templateId root="2.16.840.1.113883.10.20.22.1.2"/>
  <id root="2.16.840.1.113883.19.5" extension="{{ patient.id }}" assigningAuthorityName="Cypress Project"/>
  <code code="34133-9" displayName="Summarization of episode note" codeSystem="2.16.840.1.113883.6.1" codeSystemName="LOINC"/>
  <title>Cypress CCDA Patient Test Record: {{ patient.first }} {{ patient.last }}</title>
  <effectiveTime value="{{ date_as_number(nil) }}"/>
  <confidentialityCode code="N"/>
  <languageCode code="en-US"/>
  <recordTarget>
    <patientRole>
      <id root="Cypress" extension="{{ patient.id }}"/>
      <addr use="HP">
        <streetAddressLine>202 Burlington Rd.</streetAddressLine>
        <city>Bedford</city>
        <state>MA</state>
        <postalCode>01730</postalCode>
      </addr>
      <telecom value="tel:+1-781-271-3000"/>
      <patient>
        <name>
          <given>{{ patient.first }}</given>
          <family>{{ patient.last }}</family>
        </name>
        <administrativeGenderCode code="{{ patient.gender }}" codeSystem="2.16.840.1.113883.5.1" codeSystemName="HL7 AdministrativeGender"/>
        <birthTime value="{{ date_as_number(patient.birthdate) }}"/>
        {{#patient.race}}{{#.CDC}}
        <raceCode code="{{ . }}" {{#name}}displayName="{{ name }}"{{/name }} codeSystemName="CDC Race and Ethnicity" codeSystem="2.16.840.1.113883.6.238"/>{{/.CDC}}{{/patient.race}}
        {{#patient.ethnicity}}{{#.CDC}}
          <ethnicGroupCode code="{{ . }}" {{#name}}displayName="{{ name }}"{{/name }} codeSystemName="CDC Race and Ethnicity" codeSystem="2.16.840.1.113883.6.238"/>{{/.CDC}}{{/patient.ethnicity}}
        {{#patient.languages}}
          <languageCommunication>
            <languageCode code="{{ .IETF }}"/>
          </languageCommunication>
          {{/patient.languages}}
          {{^patient.languages}}
          <languageCommunication>
            <languageCode code="en-us"/>
          </languageCommunication>
          {{/patient.languages}}
      </patient>
    </patientRole>
  </recordTarget>
  <author>
    <time value="{{ date_as_number(nil) }}"/>
    <assignedAuthor>
      <id/>
      <addr/>
      <telecom value="tel:+1-781-271-3000"/>
      <assignedPerson>
        <name>Auto Generated</name>
      </assignedPerson>
    </assignedAuthor>
  </author>
  <custodian>
    <assignedCustodian>
      <representedCustodianOrganization>
        <id/>
        <name>Cypress</name>
        <telecom value="tel:+1-781-271-3000"/>
        <addr>
          <streetAddressLine>202 Burlington Rd.</streetAddressLine>
          <city>Bedford</city>
          <state>MA</state>
          <postalCode>01730</postalCode>
        </addr>
      </representedCustodianOrganization>
    </assignedCustodian>
  </custodian>
  <component>
    <structuredBody>

    {{#patient.allergies}}
      {{>ccda_allergies.ccda}}
    {{/patient.allergies}}
    {{^patient.allergies}}
      {{>ccda_allergies_no_current.ccda }}
    {{/patient.allergies}}

    {{#patient.medications}}
      {{>ccda_medications.ccda }}
    {{/patient.medications}}
    {{^patient.medications}}
      {{>ccda_medications_no_current.ccda }}
    {{/patient.medications}}

    {{#patient.results}}
      {{>ccda_results.ccda }}
    {{/patient.results}}

    {{#patient.conditions}}
      {{>ccda_conditions.ccda }}
    {{/patient.conditions}}
    {{^patient.conditions}}
      {{>ccda_conditions_no_current.ccda }}
    {{/patient.conditions}}

    {{#patient.procedures}}
      {{>ccda_procedures.ccda }}
    {{/patient.procedures}}

    {{#patient.encounters}}
      {{>ccda_encounters.ccda }}
    {{/patient.encounters}}

    {{#patient.vital_signs}}
      {{>ccda_vital_signs.ccda }}
    {{/patient.vital_signs}}

    {{#patient.immunizations}}
      {{>ccda_immunizations.ccda }}
    {{/patient.immunizations}}

    {{#patient.care_goals}}
      {{>ccda_care_goals.ccda }}
    {{/patient.care_goals}}

    {{#patient.social_history}}
      {{>ccda_social_history.ccda }}
    {{/patient.social_history}}

    {{#patient.medical_equipment}}
      {{>ccda_medical_equipment.ccda }}
    {{/patient.medical_equipment}}

    </structuredBody>
  </component>
</ClinicalDocument>